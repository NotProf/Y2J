{
  "name": "y2j",
  "version": "0.0.1",
  "description": "CLI for converting json to yaml and yaml to json",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "cli": "ts-node src/index.ts",
    "prettier": "npx prettier --write \"**/*.{ts,js,json}\"",
    "prettier:check": "npx prettier --check \"**/*.{ts,js,json}\""
  },
  "keywords": [
    "json",
    "yaml",
    "transform",
    "convert"
  ],
  "author": "Ihor K.",
  "license": "ISC",
  "dependencies": {
    "colorette": "^2.0.20",
    "commander": "^11.0.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "prettier": "3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
